{
  "name": "ck_targeting_frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "",
  "license": "",
  "scripts": {
    "start:dev": "next",
    "build:dev": "next build",
    "build": "NODE_ENV=production next build",
    "analyze": "NODE_ENV=production ANALYZE=true next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.10.0",
    "axios": "0.18.0",
    "classnames": "^2.2.6",
    "moment": "2.22.2",
    "next": "7.0.1",
    "next-routes": "1.4.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-ga": "2.5.3",
    "react-redux": "5.0.7",
    "react-slick": "^0.23.2",
    "reactstrap": "^6.5.0",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "webpack": "4.23.1"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.10",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/jest": "23.3.3",
    "@types/node": "8.10.14",
    "@types/react": "16.3.14",
    "@types/react-redux": "6.0.0",
    "@zeit/next-sass": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-define": "1.3.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "node-sass": "4.9.4",
    "postcss": "7.0.5",
    "postcss-preset-env": "6.0.10",
    "postcss-url": "8.0.0",
    "ts-jest": "23.10.3",
    "typescript": "3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "enzyme.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
